      <Icon>Textures\GUI\Icons\Fuelburners\kuatihydo.dds</Icon>
      <CubeSize>Large</CubeSize>
      <BlockTopology>TriangleMesh</BlockTopology>
      <Size x="1" y="1" z="2" />
