        <MountPoint Side="Bottom" StartX="0.0" StartY="0.0" EndX="1" EndY="2" Default="true" />
        <MountPoint Side="Front" StartX="0.0" StartY="0.0" EndX="1" EndY="1" />
		<MountPoint Side="Back" StartX="0.0" StartY="0.0" EndX="1" EndY="1" />
		<MountPoint Side="Left" StartX="0.0" StartY="0.0" EndX="1" EndY="2" />
		<MountPoint Side="Right" StartX="0.0" StartY="0.0" EndX="1" EndY="2" />
      </MountPoints>
