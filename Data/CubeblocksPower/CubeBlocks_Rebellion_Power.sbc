      <Components>
       <Component Subtype="LightweightDurasteel" Count="25" />
       		 <Component Subtype="ImperialConstruction" Count="24" />
		 <Component Subtype="PowerGen_Basics" Count="10" />
        <Component Subtype="StandardPowerCell" Count="6"/>
